  "dependencies": {
    "axios": "^1.7.2",
    "bunyan": "^1.8.15",
    "canvas": "^2.11.2",
    "color": "^4.2.3",
    "glob": "^11.0.0",
    "kasumi.js": "^0.6.8",
    "memory-cache": "^0.2.0",
    "prettier": "^3.3.2",
    "redis": "^4.7.0",
    "rimraf": "^5.0.8",
    "sharp": "^0.33.5",
    "string-template": "^1.0.0",
